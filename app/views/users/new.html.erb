<%= form_for(@user, method: :post, :url => {controller: "users", action: "create"}) do |f| %>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<br>
<%= f.label :password %> 
<%= f.password_field :password %>
<br>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<br>
<%= f.submit %>
<% end %>